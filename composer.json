{
  "name": "srwiez/whatsdiff",
  "description": "See what's changed in your project's dependencies",
  "type": "project",
  "license": "MIT",
  "authors": [
    {
      "name": "Eser DENIZ",
      "email": "srwiez@gmail.com"
    }
  ],
  "bin": [
    "bin/whatsdiff"
  ],
  "require": {
    "php": "^8.1",
    "ext-dom": "*",
    "composer-runtime-api": "^2.2",
    "laravel/prompts": "^0.2.0",
    "illuminate/collections": "^10.0|^11.0",
    "composer/semver": "^3.4",
    "saloonphp/saloon": "^3.10"
  },
  "require-dev": {
    "pestphp/pest": "^2.24",
    "laravel/pint": "^1.13",
    "humbug/box": "^4.3",
    "nunomaduro/collision": "^7.0|^8.0"
  },
  "config": {
    "allow-plugins": {
      "pestphp/pest-plugin": true
    }
  },
  "autoload": {
    "psr-4": {
      "App\\": "src/"
    }
  },
  "minimum-stability": "stable",
  "prefer-stable": true,
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "tests/"
    }
  },
  "scripts": {
    "build": [
      "Composer\\Config::disableProcessTimeout",
      "./build.sh"
    ],
    "qa": [
      "@composer pint",
      "@composer test"
    ],
    "box": [
      "./vendor/bin/box"
    ],
    "pint": [
      "./vendor/bin/pint --preset psr12"
    ],
    "test": [
      "./vendor/bin/pest"
    ]
  }
}
